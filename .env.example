# TVC (The Vault Club) Environment Configuration
# Copy this file to .env and fill in the actual values

# API Configuration
VITE_TVC_API_BASE=https://your-tvc-api-domain.com/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags
VITE_DISABLE_METAMASK=1
FEATURE_TESTNET_ONLY=1
SIMULATION_MODE=1

# Blockchain Configuration
CHAIN_ID=80002
VAULT_CONTRACT_ADDRESS=0xYourVaultContractAddressHere

# Wallet Configuration
VITE_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id
VITE_INFURA_API_KEY=your_infura_api_key

# Vault Club API
VITE_VAULT_CLUB_API_KEY=your_vault_club_api_key

# Development Settings
VITE_APP_ENV=development
VITE_DEBUG_MODE=false

# Security Keys (Never commit actual values)
JWT_SECRET=your_jwt_secret_here
SIGNING_SECRET=your_signing_secret_here

# Mock Data Settings (for development)
VITE_USE_MOCK_DATA=true
VITE_MOCK_VAULT_BALANCE=1000
VITE_MOCK_MEMBER_COUNT=42
